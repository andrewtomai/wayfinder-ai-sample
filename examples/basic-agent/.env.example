# Atrius Wayfinder Venue Configuration
# Get these values from your Atrius Wayfinder administrator
VITE_ATRIUS_VENUE_ID=<venue-id>
VITE_ATRIUS_ACCOUNT_ID=<api-key-or-token>

# AI Client Configuration - Google Gemini
# Get your API key from https://aistudio.google.com/apikey
VITE_AI_CLIENT_API_KEY=<your-gemini-api-key>

# Gemini model version (update to latest as needed)
VITE_AI_CLIENT_MODEL=gemini-flash-latest

# Temperature controls AI creativity (0.0 = deterministic, 1.0 = very creative)
# For venue assistance, 0.5-0.7 is recommended
VITE_AI_CLIENT_TEMPERATURE=0.7

